entity UserEntity {
    username String,
    role String
}

entity Charity {
    name String
}

entity SocialFeed {
}

entity PostEntity {
    content ImageBlob,
    timeStamp Instant,
}
entity Comment
{
    content String,
    timeStamp Integer,
    status String,
    parentId Integer
  }
relationship OneToOne
{
Charity{socialFeed} to SocialFeed
}

relationship OneToMany {

SocialFeed to PostEntity
UserEntity to PostEntity
UserEntity to Comment
Charity to Comment
Comment to Comment

}


